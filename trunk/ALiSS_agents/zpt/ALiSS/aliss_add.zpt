<html lang="en">
<head>
	<title>Add ALiSS Product</title>

	<link rel="stylesheet" type="text/css" href="/manage_page_style.css" />
	<link rel="stylesheet" type="text/css" href="manage_options_style" />
	<style>
		th {
			vertical-align:top;
			text-align:left;
			padding-right:13px;
		}
	</style>

</head>


<body>

<table width="100%" cellpadding="2" cellspacing="0" border="0">
	<tr class="location-bar">
		<td align="left" valign="top">
			<div class="form-title">Add ALiSS</div>
		</td>
	</tr>
</table>

	<br />

	<form name="frmAdd" method="post" action="manage_addALiSS">
	<table cellspacing="2" cellpadding="2" border="0">
		<tr>
			<th>Id</th>
			<td><input type="text" name="id" style="width:380px" value="" /></td>
		</tr>

		<tr>
			<th>Title</th>
			<td><input type="text" name="title" style="width:380px" /></td>
		</tr>
		<tr>
			<th>Description</th>
			<td><textarea name="description" rows="6" style="width:380px"></textarea></td>
		</tr>
		<tr>
			<th>&nbsp;</th>
			<td><input type="submit" value="Add" style="padding:0em 0.5em;" /></td>
		</tr>
	</table>
	</form>

</body>
</html>
